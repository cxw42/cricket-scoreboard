  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/cxw42/cricket-scoreboard/issues"
  },
  "homepage": "https://github.com/cxw42/cricket-scoreboard#readme",
  "dependencies": {
    "snapsvg": "^0.5.1",
    "two.js": "^0.8.10",
    "wcag-contrast": "^3.0.0"
  },
  "devDependencies": {
    "brunch": "^4.0.1",
    "js-beautify": "^1.14.7",
    "yuidocjs": "^0.10.2"
  }
}
