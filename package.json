{
  "name": "cricket-scoreboard",
  "version": "0.0.1",
  "description": "A cricket scoreboard OSD for a US audience",
  "main": "index.js",
  "scripts": {
    "start": "npx brunch watch -s",
    "doc": "npx yuidoc -o doc -x .git,public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "p": "find app -name \\*.js -exec npx js-beautify -r --editorconfig -w 80 {} + ; npx js-beautify -r --editorconfig -w 80 brunch-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cxw42/cricket-scoreboard.git"
  },
